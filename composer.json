{
    "name": "dannidickson/dex",
    "description": "Dex - Custom data export and reporting module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "marczhermo/federer": "5.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "repositories": [
        {
           "type": "git",
           "url": "git@github.com:dannidickson/federer.git"
        }
   ],
    "autoload": {
        "psr-4": {
            "Dex\\": "src/",
            "Dex\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
